{
  "name": "hubot-ringcentral-sms",
  "version": "0.2.0",
  "description": "RingCentral SMS Adapter for Hubot",
  "scripts": {
    "pretest": "coffeelint ./src",
    "test": "npm run mocha && istanbul report text-summary lcov",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "mocha": "mocha --compilers coffee:coffee-script/register  --require coffee-coverage/register-istanbul --reporter spec test",
    "preversion": "npm test",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags && rm -rf build/temp"
  },
  "main": "./ringcentralSMS",
  "engines": {
    "node": ">=4.6.x",
    "npm": ">=3.10.x"
  },
  "dependencies": {
    "parent-require": "^1.0.0",
    "ringcentral": "^3.0.0-rc1"
  },
  "devDependencies": {
    "coffee-coverage": "^1.0.1",
    "coffee-script": ">=1.2.0",
    "coffeelint": "^1.15.7",
    "coveralls": "^2.11.14",
    "hubot": ">=2.19.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.0",
    "should": "^11.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LingForCC/hubot-ringcentral-sms.git"
  },
  "author": {
    "name": "RingCentral, Inc.",
    "url": "https://developers.ringcentral.com"
  },
  "contributors": [
    {
      "name": "Colin Liu",
      "email": "colin.liu@ringcentral.com",
      "url": "https://github.com/LingForCC"
    },
    {
      "name": "Benjamin Dean",
      "email": "benjamin.dean@ringcentral.com",
      "url": "https://github.com/bdeanindy"
    }
  ],
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/LingForCC/hubot-ringcentral-sms/issues"
  },
  "homepage": "https://github.com/LingForCC/hubot-ringcentral-sms#readme"
}
